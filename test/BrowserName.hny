COALESCE(
	IF(
		REG_MATCH(
			$request.header.user_agent,
			"Gecko/"
		),
		"Firefox"
	),
	IF(
		OR(
			REG_MATCH(
				$request.header.user_agent,
				"Chrome/"
			),
			REG_MATCH(
				$request.header.user_agent,
				"Chromium/"
			)
		),
		"Chrome"
	),
	IF(
		REG_MATCH(
			$request.header.user_agent,
			"AppleWebKit/"
		),
		"Webkit"
	),
	IF(
		REG_MATCH(
			$request.header.user_agent,
			"Trident/"
		),
		"IE"
	),
	IF(
		REG_MATCH(
			$request.header.user_agent,
			"Edge/"
		),
		"Edge"
	),
	"Unknown"
)
